/* components/goods/good.wxss */
.good-item {
  text-align: center;
  padding: 16rpx 0;
  font-size: 24rpx;
}
.good-item image {
  max-width: 100%;
  height: 550rpx;
  border-radius: 10rpx;
}
.good-item .title {
  padding: 10rpx 0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.good-item .price {
  font-size: 28rpx;
  color: #ff5777;
}